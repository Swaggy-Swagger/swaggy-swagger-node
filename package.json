{
	"name": "swaggy-swagger",
	"version": "1.0.0",
	"description": "A custom Swagger UI package with Node and TypeScript",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc && npm run postbuild",
		"postbuild": "copyfiles -u 1 src/*.d.ts dist"
	},
	"files": [
		"dist",
		"public"
	],
	"dependencies": {
		"@types/swagger-jsdoc": "^6.0.4",
		"express": "^4.x.x",
		"swagger-jsdoc": "^6.x.x"
	},
	"devDependencies": {
		"@types/express": "^4.x.x",
		"copyfiles": "^2.4.1",
		"typescript": "^4.x.x"
	},
	"repository": "https://github.com/Swaggy-Swagger/swaggy-swagger-node",
	"license": "Apache-2.0"
}
